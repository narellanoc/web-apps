<% 
companies = Company.all
%>

Companies

<ul>
<% for company in companies %>
    <li><%= company["name"] %></li>
<% end %>
</ul>

<table>
    <tr>
        <th>Name</th>
        <th>City</th>
        <th>State</th>
    </tr>
    <% for company in companies %>
    <tr>
        <td><%= company["name"]%></td>
        <td><%= company["city"]%></td>
        <td><%= company["state"]%></td>
    </tr>
    <% end %>
</table>
